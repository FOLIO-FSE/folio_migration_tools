            "items": {
                "type": "string"
            }
        },
        "reset_hrid_settings": {
            "title": "Reset Hrid Settings",
            "default": false,
            "type": "boolean"
        },
        "never_update_hrid_settings": {
            "title": "Never Update Hrid Settings",
            "default": false,
            "type": "boolean"
        }
