            "type": "boolean"
        },
        "add_administrative_notes_with_legacy_ids": {
            "title": "Add Administrative Notes With Legacy Ids",
            "default": true,
            "type": "boolean"
        },
        "migration_task_type": {
            "title": "Migration Task Type",
            "type": "string"
        },
        "use_tenant_mapping_rules": {
            "title": "Use Tenant Mapping Rules",
            "default": true,
            "type": "boolean"
        },
        "hrid_handling": {
            "default": "default",
            "allOf": [
                {
                    "$ref": "#/definitions/HridHandling"
                }
            ]
        },
        "files": {
