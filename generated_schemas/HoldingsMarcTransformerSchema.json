{
    "title": "TaskConfiguration",
    "type": "object",
    "properties": {
        "name": {
            "title": "Name",
            "type": "string"
        },
        "legacyIdMarcPath": {
            "title": "Legacyidmarcpath",
            "type": "string"
        },
        "deduplicateHoldingsStatements": {
            "title": "Deduplicateholdingsstatements",
            "default": true,
            "type": "boolean"
        },
        "migrationTaskType": {
            "title": "Migrationtasktype",
            "type": "string"
        },
        "hridHandling": {
            "default": "default",
            "allOf": [
                {
                    "$ref": "#/definitions/HridHandling"
                }
            ]
        },
        "deactivate035From001": {
            "title": "Deactivate035From001",
            "default": false,
            "type": "boolean"
        },
        "files": {
            "title": "Files",
            "type": "array",
            "items": {
                "$ref": "#/definitions/FileDefinition"
            }
        },
        "locationMapFileName": {
            "title": "Locationmapfilename",
            "type": "string"
        },
        "defaultCallNumberTypeName": {
            "title": "Defaultcallnumbertypename",
            "type": "string"
        },
        "fallbackHoldingsTypeId": {
            "title": "Fallbackholdingstypeid",
            "type": "string"
        },
        "holdingsTypeUuidForBoundwiths": {
            "title": "Holdings Type for Boundwith Holdings",
            "description": "UUID for a Holdings type (set in Settings->Inventory) for Bound-with Holdings)",
            "default": "",
            "type": "string"
        },
        "boundwithRelationshipFilePath": {
            "title": "Boundwith relationship file path",
            "description": "Path to a file outlining Boundwith relationships, in the style of Voyager. A TSV file with MFHD_ID and BIB_ID headers and values",
            "default": "",
            "type": "string"
        },
        "createSourceRecords": {
            "title": "Createsourcerecords",
            "description": "Controls wheter or not to retain the MARC records in SRS.",
            "default": true,
            "type": "boolean"
        },
        "resetHridSettings": {
            "title": "Resethridsettings",
            "default": false,
            "type": "boolean"
        },
        "neverUpdateHridSettings": {
            "title": "Neverupdatehridsettings",
            "default": false,
            "type": "boolean"
        }
    },
    "required": [
        "name",
        "legacyIdMarcPath",
        "migrationTaskType",
        "files",
        "locationMapFileName",
        "defaultCallNumberTypeName",
        "fallbackHoldingsTypeId"
    ],
    "definitions": {
        "HridHandling": {
            "title": "HridHandling",
            "description": "Enum determining how the HRID generation should be handled.\n    - default: Enumerates the HRID, building on the current value in the HRID settings\n    - preserve001: Takes the 001 and uses this as the HRID.\n\nArgs:\n    str (_type_): _description_\n    Enum (_type_): _description_",
            "enum": [
                "default",
                "preserve001"
            ],
            "type": "string"
        },
        "FileDefinition": {
            "title": "FileDefinition",
            "type": "object",
            "properties": {
                "file_name": {
                    "title": "File Name",
                    "type": "string"
                },
                "suppressed": {
                    "title": "Suppressed",
                    "default": false,
                    "type": "boolean"
                },
                "staff_suppressed": {
                    "title": "Staff Suppressed",
                    "default": false,
                    "type": "boolean"
                },
                "service_point_id": {
                    "title": "Service Point Id",
                    "default": "",
                    "type": "string"
                }
            },
            "required": [
                "file_name"
            ]
        }
    }
}
